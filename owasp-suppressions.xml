<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- 
    OWASP Dependency Check Suppressions for JiraInsight Desktop
    
    This file contains suppressions for false positives and accepted risks
    in the dependency vulnerability scanning process.
    
    Each suppression should include:
    - Clear reason for suppression
    - CVE ID or vulnerability pattern
    - Affected files/dependencies
    - Review date and reviewer
    -->
    
    <!-- Example suppressions - remove or modify as needed -->
    
    <!-- Suppress JavaFX false positives -->
    <suppress>
        <notes><![CDATA[
        JavaFX runtime dependencies may trigger false positives for web-related CVEs
        that don't apply to desktop applications.
        Reviewed: 2025-08-06
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.openjfx/.*$</packageUrl>
        <cve>CVE-2021-44228</cve> <!-- Log4j - not applicable to JavaFX -->
    </suppress>
    
    <!-- Suppress test-only dependencies -->
    <suppress>
        <notes><![CDATA[
        Test dependencies are not included in the final application distribution
        and pose no runtime security risk.
        Reviewed: 2025-08-06
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.junit\..*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Mockito test framework - not included in production builds
        Reviewed: 2025-08-06
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.mockito/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Suppress Maven plugin vulnerabilities -->
    <suppress>
        <notes><![CDATA[
        Maven plugins are build-time only and not included in the final application.
        These vulnerabilities do not affect the runtime security of the application.
        Reviewed: 2025-08-06
        ]]></notes>
        <packageUrl regex="true">^pkg:maven/org\.apache\.maven\.plugins/.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
